or product; proposed levels: Beginner, Intermediate, Advanced.""" ;
                                                             
                                                             rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                             
                                                             :propertyDisjointWith <http://blankdots.com/open/personasonto.owl#technicalLevel> ;
                                                             
                                                             rdfs:range rdfs:Literal .



###  http://blankdots.com/open/personasonto.owl#familyName

<http://blankdots.com/open/personasonto.owl#familyName> rdf:type :DatatypeProperty ,
                                                                 :FunctionalProperty ;
                                                        
                                                        dc:description "Family or Last name of a person." ;
                                                        
                                                        rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                        
                                                        :propertyDisjointWith <http://blankdots.com/open/personasonto.owl#firstName> ;
                                                        
                                                        rdfs:range xsd:string .



###  http://blankdots.com/open/personasonto.owl#firstName

<http://blankdots.com/open/personasonto.owl#firstName> rdf:type :DatatypeProperty ,
                                                                :FunctionalProperty ;
                                                       
                                                       dc:description "First or Given name of a person." ;
                                                       
                                                       rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                       
                                                       rdfs:range xsd:string .



###  http://blankdots.com/open/personasonto.owl#frustrationPoint

<http://blankdots.com/open/personasonto.owl#frustrationPoint> rdf:type :DatatypeProperty ;
                                                              
                                                              dc:description "Elements of a UI/UX or certain characteristics that frustrate the user or (s)he sees as pain points." ;
                                                              
                                                              rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                              
                                                              rdfs:range rdfs:Literal .



###  http://blankdots.com/open/personasonto.owl#gender

<http://blankdots.com/open/personasonto.owl#gender> rdf:type :DatatypeProperty ,
                                                             :FunctionalProperty ;
                                                    
                                                    dc:description "Gender of a person." ;
                                                    
                                                    rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                    
                                                    rdfs:range rdfs:Literal .



###  http://blankdots.com/open/personasonto.owl#interactionCount

<http://blankdots.com/open/personasonto.owl#interactionCount> rdf:type :DatatypeProperty ;
                                                              
