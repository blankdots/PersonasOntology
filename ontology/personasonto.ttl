@base <http://blankdots.com/open/personasonto.owl> .

<http://blankdots.com/open/personasonto.owl> rdf:type :Ontology ;
                                             
                                             :versionInfo "1.5"^^rdfs:Literal ;
                                             
                                             dc:date "2015-05-24"^^xsd:dateTime ;
                                             
                                             rdfs:seeAlso "http://blankdots.com/open/schema" ;
                                             
                                             rdfs:comment "PersonasOnto ontology incorporates concepts and properties used to model personas."@en ;
                                             
                                             dc:rights "This ontology is distributed under Attribution-ShareAlike 4.0 Unported - http://creativecommons.org/licenses/by-sa/4.0/" ;
                                             
                                             dc:creator "Stefan Negru" ;
                                             
                                             dc:contributor "Sabin Buraga" ;
                                             
                                             dc:title "PersonasOnto"@en ;
                                             
                                             dc:description """The PersonasOnto is depicts the personas domain along with relating concept and properties. A persona is regarded as a user archetype which can be used to \"help guide decisions about product features, navigation, interactions, and even visual design\" (Goodwin, 2005).

This ontology wants to address some of the issues regarding this methodology like how personas are reconciled with other information and who is responsible for interpreting and validating them."""@en ;
                                             
                                             :imports <http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl> .


#################################################################
#
#    Object Properties
#
#################################################################


###  http://blankdots.com/open/personasonto.owl#dependsOn

<http://blankdots.com/open/personasonto.owl#dependsOn> rdf:type :ObjectProperty ;
                                                       
                                                       dc:description "The completion of a Task might depend on another Task, as several Tasks are part of a Scenario." ;
                                                       
                                                       rdfs:domain <http://blankdots.com/open/personasonto.owl#Task> ;
                                                       
                                                       rdfs:range <http://blankdots.com/open/personasonto.owl#Task> .



###  http://blankdots.com/open/personasonto.owl#depiction

<http://blankdots.com/open/personasonto.owl#depiction> rdf:type :ObjectProperty ;
                                                       
                                                       dc:description "A certain resource might depict a/any thing." ;
                                                       
                                                       rdfs:range <http://blankdots.com/open/personasonto.owl#Resource> ;
                                                       
                                                       rdfs:domain :Thing .



###  http://blankdots.com/open/personasonto.owl#depictsDocument

<http://blankdots.com/open/personasonto.owl#depictsDocument> rdf:type :ObjectProperty ;
                                                             
                                                             rdfs:range <http://blankdots.com/open/personasonto.owl#Document> ;
                                                             
                                                             rdfs:subPropertyOf <http://blankdots.com/open/personasonto.owl#depiction> ;
                                                             
                                                             rdfs:domain :Thing .



###  http://blankdots.com/open/personasonto.owl#depictsImage

<http://blankdots.com/open/personasonto.owl#depictsImage> rdf:type :ObjectProperty ;
                                                          
                                                          rdfs:range <http://blankdots.com/open/personasonto.owl#Image> ;
                                                          
                                                          rdfs:subPropertyOf <http://blankdots.com/open/personasonto.owl#depiction> ;
                                                          
                                                          rdfs:domain :Thing .



###  http://blankdots.com/open/personasonto.owl#depictsProduct

<http://blankdots.com/open/personasonto.owl#depictsProduct> rdf:type :ObjectProperty ;
                                                            
                                                            rdfs:range <http://blankdots.com/open/personasonto.owl#Product> ;
                                                            
                                                            rdfs:subPropertyOf <http://blankdots.com/open/personasonto.owl#depiction> ;
                                                            
                                                            rdfs:domain :Thing .



###  http://blankdots.com/open/personasonto.owl#hasAffectiveState

<http://blankdots.com/open/personasonto.owl#hasAffectiveState> rdf:type :ObjectProperty ;
                                                               
                                                               dc:description "A person might have a certain affective state on a given moment or in a given context." ;
                                                               
                                                               rdfs:range <http://blankdots.com/open/personasonto.owl#AffectiveState> ;
                                                               
                                                               rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> .



###  http://blankdots.com/open/personasonto.owl#hasAffiliation

<http://blankdots.com/open/personasonto.owl#hasAffiliation> rdf:type :ObjectProperty ;
                                                            
                                                            dc:description "A person might have a certain type of affiliation to an organization and might not be member of it." ;
                                                            
                                                            rdfs:range <http://blankdots.com/open/personasonto.owl#Organization> ;
                                                            
                                                            rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> .



###  http://blankdots.com/open/personasonto.owl#hasAward

<http://blankdots.com/open/personasonto.owl#hasAward> rdf:type :ObjectProperty ;
                                                      
                                                      dc:description "A person might have been awarded with some sort of distinction (depicted usually by a trophy or document)." ;
                                                      
                                                      rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                      
                                                      rdfs:range <http://blankdots.com/open/personasonto.owl#Resource> .



###  http://blankdots.com/open/personasonto.owl#hasBusinessGoal

<http://blankdots.com/open/personasonto.owl#hasBusinessGoal> rdf:type :ObjectProperty ;
                                                             
                                                             rdfs:range <http://blankdots.com/open/personasonto.owl#BusinessGoals> ;
                                                             
                                                             rdfs:subPropertyOf <http://blankdots.com/open/personasonto.owl#hasGoal> .



###  http://blankdots.com/open/personasonto.owl#hasContext

<http://blankdots.com/open/personasonto.owl#hasContext> rdf:type :ObjectProperty ;
                                                        
                                                        dc:description "A scenario is characterized by a certain context." ;
                                                        
                                                        rdfs:range <http://blankdots.com/open/personasonto.owl#Context> ;
                                                        
                                                        rdfs:domain <http://blankdots.com/open/personasonto.owl#Scenario> .



###  http://blankdots.com/open/personasonto.owl#hasDisability

<http://blankdots.com/open/personasonto.owl#hasDisability> rdf:type :ObjectProperty ;
                                                           
                                                           dc:description "A person might have a disability." ;
                                                           
                                                           rdfs:range <http://blankdots.com/open/personasonto.owl#Disability> ;
                                                           
                                                           rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> .



###  http://blankdots.com/open/personasonto.owl#hasEndGoal

<http://blankdots.com/open/personasonto.owl#hasEndGoal> rdf:type :ObjectProperty ;
                                                        
                                                        rdfs:range <http://blankdots.com/open/personasonto.owl#EndGoals> ;
                                                        
                                                        rdfs:subPropertyOf <http://blankdots.com/open/personasonto.owl#hasGoal> .



###  http://blankdots.com/open/personasonto.owl#hasExperienceGoal

<http://blankdots.com/open/personasonto.owl#hasExperienceGoal> rdf:type :ObjectProperty ;
                                                               
                                                               rdfs:range <http://blankdots.com/open/personasonto.owl#ExperienceGoals> ;
                                                               
                                                               rdfs:subPropertyOf <http://blankdots.com/open/personasonto.owl#hasGoal> .



###  http://blankdots.com/open/personasonto.owl#hasGoal

<http://blankdots.com/open/personasonto.owl#hasGoal> rdf:type :ObjectProperty ;
                                                     
                                                     dc:description "A person might have one or more (short/long term) goals depending on the context." ;
                                                     
                                                     rdfs:range <http://blankdots.com/open/personasonto.owl#Goals> ;
                                                     
                                                     rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                     
                                                     :inverseOf <http://blankdots.com/open/personasonto.owl#isGoalOf> .



###  http://blankdots.com/open/personasonto.owl#hasInfluence

<http://blankdots.com/open/personasonto.owl#hasInfluence> rdf:type :ObjectProperty ;
                                                          
                                                          dc:description "The disability of a person migh have an effect on the affective state or personality on the person." ;
                                                          
                                                          rdfs:domain <http://blankdots.com/open/personasonto.owl#Disability> ;
                                                          
                                                          rdfs:range [ rdf:type :Class ;
                                                                       :unionOf ( <http://blankdots.com/open/personasonto.owl#AffectiveState>
                                                                                  <http://blankdots.com/open/personasonto.owl#Personality>
                                                                                )
                                                                     ] .



###  http://blankdots.com/open/personasonto.owl#hasInterest

<http://blankdots.com/open/personasonto.owl#hasInterest> rdf:type :ObjectProperty ;
                                                         
                                                         dc:description "A person might be interested in a certain resource (product, application, document)." ;
                                                         
                                                         rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                         
                                                         rdfs:range <http://blankdots.com/open/personasonto.owl#Resource> .



###  http://blankdots.com/open/personasonto.owl#hasLifeGoal

<http://blankdots.com/open/personasonto.owl#hasLifeGoal> rdf:type :ObjectProperty ;
                                                         
                                                         rdfs:range <http://blankdots.com/open/personasonto.owl#LifeGoals> ;
                                                         
                                                         rdfs:subPropertyOf <http://blankdots.com/open/personasonto.owl#hasGoal> .



###  http://blankdots.com/open/personasonto.owl#hasPersonaType

<http://blankdots.com/open/personasonto.owl#hasPersonaType> rdf:type :ObjectProperty ;
                                                            
                                                            dc:description "The type of Persona: Primary, Secondary, Negative, Supplemental, Served or Customer." ;
                                                            
                                                            rdfs:range <http://blankdots.com/open/personasonto.owl#PersonaType> ;
                                                            
                                                            rdfs:domain [ rdf:type :Class ;
                                                                          :unionOf ( <http://blankdots.com/open/personasonto.owl#Participant>
                                                                                     <http://blankdots.com/open/personasonto.owl#Person>
                                                                                   )
                                                                        ] .



###  http://blankdots.com/open/personasonto.owl#hasPersonality

<http://blankdots.com/open/personasonto.owl#hasPersonality> rdf:type :ObjectProperty ;
                                                            
                                                            dc:description "A person has a certain personality corresponding to Myers Briggs personality classification." ;
                                                            
                                                            rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                            
                                                            rdfs:range <http://blankdots.com/open/personasonto.owl#Personality> .



###  http://blankdots.com/open/personasonto.owl#hasPublications

<http://blankdots.com/open/personasonto.owl#hasPublications> rdf:type :ObjectProperty ;
                                                             
                                                             dc:description "A person (of academic, journalistic profession) has published several documents." ;
                                                             
                                                             rdfs:range <http://blankdots.com/open/personasonto.owl#Document> ;
                                                             
                                                             rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> .



###  http://blankdots.com/open/personasonto.owl#hasReview

<http://blankdots.com/open/personasonto.owl#hasReview> rdf:type :ObjectProperty ;
                                                       
                                                       dc:description "A resource, scenario or usability test is described, reviewed via a document." ;
                                                       
                                                       rdfs:range <http://blankdots.com/open/personasonto.owl#Document> ;
                                                       
                                                       rdfs:domain [ rdf:type :Class ;
                                                                     :unionOf ( <http://blankdots.com/open/personasonto.owl#Resource>
                                                                                <http://blankdots.com/open/personasonto.owl#Scenario>
                                                                                <http://blankdots.com/open/personasonto.owl#UsabilityTest>
                                                                              )
                                                                   ] .



###  http://blankdots.com/open/personasonto.owl#hasTask

<http://blankdots.com/open/personasonto.owl#hasTask> rdf:type :ObjectProperty ;
                                                     
                                                     dc:description "A scenario has a set of tasks (or just one). (inverseproperty isTaskfor)" ;
                                                     
                                                     rdfs:domain <http://blankdots.com/open/personasonto.owl#Scenario> ;
                                                     
                                                     rdfs:range <http://blankdots.com/open/personasonto.owl#Task> .



###  http://blankdots.com/open/personasonto.owl#hasTechnicalGoal

<http://blankdots.com/open/personasonto.owl#hasTechnicalGoal> rdf:type :ObjectProperty ;
                                                              
                                                              rdfs:range <http://blankdots.com/open/personasonto.owl#TechnicalGoals> ;
                                                              
                                                              rdfs:subPropertyOf <http://blankdots.com/open/personasonto.owl#hasGoal> .



###  http://blankdots.com/open/personasonto.owl#hasTopicOfInterest

<http://blankdots.com/open/personasonto.owl#hasTopicOfInterest> rdf:type :ObjectProperty ;
                                                                
                                                                dc:description "A person might have a certain topic of interest which is correspondent to a cetain resource." ;
                                                                
                                                                rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                                
                                                                rdfs:range <http://blankdots.com/open/personasonto.owl#Resource> .



###  http://blankdots.com/open/personasonto.owl#includes

<http://blankdots.com/open/personasonto.owl#includes> rdf:type :ObjectProperty ;
                                                      
                                                      dc:description "In order to describe context we consider the medium of interaction Tangible or Intangible which is included in a Place and/or Time." ;
                                                      
                                                      rdfs:domain [ rdf:type :Class ;
                                                                    :unionOf ( <http://blankdots.com/open/personasonto.owl#Intangible>
                                                                               <http://blankdots.com/open/personasonto.owl#Tangible>
                                                                             )
                                                                  ] ;
                                                      
                                                      rdfs:range [ rdf:type :Class ;
                                                                   :unionOf ( <http://blankdots.com/open/personasonto.owl#Place>
                                                                              <http://blankdots.com/open/personasonto.owl#Time>
                                                                            )
                                                                 ] .



###  http://blankdots.com/open/personasonto.owl#influencedBy

<http://blankdots.com/open/personasonto.owl#influencedBy> rdf:type :ObjectProperty ;
                                                          
                                                          dc:description "The affective state of a person is influenced by the personality of that person. (inverse property influences)" ;
                                                          
                                                          rdfs:domain <http://blankdots.com/open/personasonto.owl#AffectiveState> ;
                                                          
                                                          rdfs:range <http://blankdots.com/open/personasonto.owl#Personality> ;
                                                          
                                                          :inverseOf <http://blankdots.com/open/personasonto.owl#influences> .



###  http://blankdots.com/open/personasonto.owl#influences

<http://blankdots.com/open/personasonto.owl#influences> rdf:type :ObjectProperty ;
                                                        
                                                        dc:description "The personality influences the affective state o a person." ;
                                                        
                                                        rdfs:range <http://blankdots.com/open/personasonto.owl#AffectiveState> ;
                                                        
                                                        rdfs:domain <http://blankdots.com/open/personasonto.owl#Personality> .



###  http://blankdots.com/open/personasonto.owl#involvedIn

<http://blankdots.com/open/personasonto.owl#involvedIn> rdf:type :ObjectProperty ;
                                                        
                                                        dc:description "A participant (a certain user or observer) is involvedIn a scenario or Usability Test." ;
                                                        
                                                        rdfs:domain <http://blankdots.com/open/personasonto.owl#Participant> ;
                                                        
                                                        rdfs:range [ rdf:type :Class ;
                                                                     :unionOf ( <http://blankdots.com/open/personasonto.owl#Scenario>
                                                                                <http://blankdots.com/open/personasonto.owl#UsabilityTest>
                                                                              )
                                                                   ] .



###  http://blankdots.com/open/personasonto.owl#isContextOf

<http://blankdots.com/open/personasonto.owl#isContextOf> rdf:type :ObjectProperty ;
                                                         
                                                         dc:description "Inverse property of hasContext, isContextof represents the context belonging to a scenario." ;
                                                         
                                                         rdfs:domain <http://blankdots.com/open/personasonto.owl#Context> ;
                                                         
                                                         rdfs:range <http://blankdots.com/open/personasonto.owl#Scenario> ;
                                                         
                                                         :inverseOf <http://blankdots.com/open/personasonto.owl#hasContext> .



###  http://blankdots.com/open/personasonto.owl#isGoalOf

<http://blankdots.com/open/personasonto.owl#isGoalOf> rdf:type :ObjectProperty ;
                                                      
                                                      dc:description "Inverse property of hasGoal, isGoalof represents the goal belonging to a person." ;
                                                      
                                                      rdfs:domain <http://blankdots.com/open/personasonto.owl#Goals> ;
                                                      
                                                      rdfs:range <http://blankdots.com/open/personasonto.owl#Person> .



###  http://blankdots.com/open/personasonto.owl#isMemberOf

<http://blankdots.com/open/personasonto.owl#isMemberOf> rdf:type :ObjectProperty ;
                                                        
                                                        dc:description "A person might be memeber of an organization." ;
                                                        
                                                        rdfs:range <http://blankdots.com/open/personasonto.owl#Organization> ;
                                                        
                                                        rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> .



###  http://blankdots.com/open/personasonto.owl#isPartOfTest

<http://blankdots.com/open/personasonto.owl#isPartOfTest> rdf:type :ObjectProperty ;
                                                          
                                                          dc:description "Usability tests have participants (observers and users), tasks and a scenario." ;
                                                          
                                                          rdfs:range <http://blankdots.com/open/personasonto.owl#UsabilityTest> ;
                                                          
                                                          rdfs:domain [ rdf:type :Class ;
                                                                        :unionOf ( <http://blankdots.com/open/personasonto.owl#Participant>
                                                                                   <http://blankdots.com/open/personasonto.owl#Scenario>
                                                                                   <http://blankdots.com/open/personasonto.owl#Task>
                                                                                 )
                                                                      ] .



###  http://blankdots.com/open/personasonto.owl#isTaskFor

<http://blankdots.com/open/personasonto.owl#isTaskFor> rdf:type :ObjectProperty ;
                                                       
                                                       dc:description "A certain task (or a series of tasks) is (are) part of a scenario." ;
                                                       
                                                       rdfs:range <http://blankdots.com/open/personasonto.owl#Scenario> ;
                                                       
                                                       rdfs:domain <http://blankdots.com/open/personasonto.owl#Task> ;
                                                       
                                                       :inverseOf <http://blankdots.com/open/personasonto.owl#hasTask> .



###  http://blankdots.com/open/personasonto.owl#knows

<http://blankdots.com/open/personasonto.owl#knows> rdf:type :ObjectProperty ;
                                                   
                                                   dc:description "A person knows another person (similar to FOAF ontology)." ;
                                                   
                                                   rdfs:range <http://blankdots.com/open/personasonto.owl#Person> ;
                                                   
                                                   rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> .



###  http://blankdots.com/open/personasonto.owl#performs

<http://blankdots.com/open/personasonto.owl#performs> rdf:type :ObjectProperty ;
                                                      
                                                      dc:description "A person performs a task in the usability test or when using a product/application." ;
                                                      
                                                      rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                      
                                                      rdfs:range <http://blankdots.com/open/personasonto.owl#Task> .



###  http://blankdots.com/open/personasonto.owl#uses

<http://blankdots.com/open/personasonto.owl#uses> rdf:type :ObjectProperty ;
                                                  
                                                  dc:description "A person makes use of a resource (document, image, product)." ;
                                                  
                                                  rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                  
                                                  rdfs:range <http://blankdots.com/open/personasonto.owl#Resource> .





