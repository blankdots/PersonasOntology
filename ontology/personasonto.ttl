#
#################################################################


###  http://blankdots.com/open/personasonto.owl#age

<http://blankdots.com/open/personasonto.owl#age> rdf:type :DatatypeProperty ,
                                                          :FunctionalProperty ;
                                                 
                                                 dc:description "Age of a person." ;
                                                 
                                                 rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                 
                                                 rdfs:range xsd:float .



###  http://blankdots.com/open/personasonto.owl#birthday

<http://blankdots.com/open/personasonto.owl#birthday> rdf:type :DatatypeProperty ;
                                                      
                                                      dc:description "Date of birth of a person." ;
                                                      
                                                      rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                      
                                                      rdfs:range xsd:dateTime .



###  http://blankdots.com/open/personasonto.owl#details

<http://blankdots.com/open/personasonto.owl#details> rdf:type :DatatypeProperty ,
                                                              :FunctionalProperty ;
                                                     
                                                     dc:description "Any kind of details." ;
                                                     
                                                     rdfs:range rdfs:Literal ;
                                                     
                                                     rdfs:domain :Thing .



###  http://blankdots.com/open/personasonto.owl#experienceLevel

<http://blankdots.com/open/personasonto.owl#experienceLevel> rdf:type :DatatypeProperty ,
                                                                      :FunctionalProperty ;
                                                             
                                                             rdfs:comment "It is different from technical level, because a person might be experienced in some product or application but does not have technical skills." ;
                                                             
