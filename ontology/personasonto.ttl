                                                      rdfs:range xsd:string ;
                                                      
                                                      rdfs:domain [ rdf:type :Class ;
                                                                    :unionOf ( <http://blankdots.com/open/personasonto.owl#Organization>
                                                                               <http://blankdots.com/open/personasonto.owl#Place>
                                                                             )
                                                                  ] .



###  http://blankdots.com/open/personasonto.owl#mainPoint

<http://blankdots.com/open/personasonto.owl#mainPoint> rdf:type :DatatypeProperty ;
                                                       
                                                       dc:description "Main points are specific elements to each persona category." ;
                                                       
                                                       rdfs:domain <http://blankdots.com/open/personasonto.owl#Person> ;
                                                       
                                                       rdfs:range rdfs:Literal .



###  http://blankdots.com/open/personasonto.owl#maxBustSize

<http://blankdots.com/open/personasonto.owl#maxBustSize> rdf:type :DatatypeProperty ,
                                                                  :FunctionalProperty ;
                                                         
                                                         dc:description "Maximum Bust Size." ;
                                                         
                                                         rdfs:domain <http://blankdots.com/open/personasonto.owl#Persona> ;
                                                         
                                                         rdfs:range xsd:positiveInteger .



###  http://blankdots.com/open/personasonto.owl#maxFeetSize

